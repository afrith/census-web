{
  "name": "census-web",
  "version": "0.1.0",
  "description": "Census info website",
  "main": "dist/index.js",
  "scripts": {
    "dev": "source environment && yarn nodemon -e js,ejs --exec babel-node src/index.js",
    "build": "yarn babel src/ -d dist/ --copy-files --ignore 'src/static/*.js'",
    "clean": "yarn rimraf dist/",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Adrian Frith <adrian@adrianfrith.com>",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.10.0",
    "koa-ejs": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.19",
    "pg": "^7.12.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": "^10.16.3",
    "yarn": "^1.19.0"
  }
}
